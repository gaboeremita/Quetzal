{
    "name": "gaboeremita/quetzal",
    "description": "Quetzal provides an API to integrate your laravel application with Constant Contact",
    "license": "MIT",
    "authors": [
        {
            "name": "Jovan Gabriel Ontiveros Perea",
            "email": "gabrieleremita@gmail.com",
            "homepage": "https://github.com/gaboeremita/"
        }
    ],
    "homepage": "https://github.com/gaboeremita/quetzal",
    "keywords": ["Laravel", "quetzal", "newsletter", "email", "listing"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "gaboeremita\\quetzal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "gaboeremita\\quetzal\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "gaboeremita\\quetzal\\quetzalServiceProvider"
            ],
            "aliases": {
                "quetzal": "gaboeremita\\quetzal\\Facades\\quetzal"
            }
        }
    }
}
